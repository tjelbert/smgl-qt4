conflicts kdelibs4-experimental y
conflicts polkit-qt y

#
# Need to remove kdebase-workspace4 4.3 before building kdelibs4 4.4
# Bug #15573
#
if spell_ok kdebase-workspace4 && [[ "$(installed_version kdebase-workspace4|cut -d. -f1,2)" == "4.3" ]]
then
  conflicts kdebase-workspace4 y
fi

#
# Conflict with self if it is installed in /opt/kde4
#
if $(gaze install kdelibs4 |grep -q /opt/kde4); then
 conflicts $SPELL y
fi


