           SPELL=kdevplatform4
          SPELLX=${SPELL//4}
         VERSION=x
      PATCHLEVEL=1
              VX=4${VERSION:1:4}
     SOURCE_HASH=sha512:
          SOURCE=$SPELLX-$VERSION.tar.bz2
   SOURCE_URL[0]=$KDE_URL/stable/kdevelop/$VX/src/$SOURCE
